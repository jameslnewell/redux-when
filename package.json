{
  "name": "redux-when",
  "version": "2.1.3",
  "description": "Redux middleware for delaying dispatch of an action until a condition evaluates to true.",
  "keywords": [
    "redux",
    "wait",
    "when",
    "until",
    "for",
    "delay",
    "once"
  ],
  "main": "./dist/index.js",
  "types": "./index.d.ts",
  "repository": "jameslnewell/redux-when",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "chai": "^4.2.0",
    "mocha": "^10.2.0",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "sinon": "^9.0.0",
    "sinon-chai": "^3.5.0"
  },
  "scripts": {
    "build": "babel ./index.js -d ./dist/",
    "watch": "npm run build -- --watch",
    "test": "mocha --require @babel/register",
    "example": "babel-node ./example/index.js",
    "prepublish": "npm run build"
  }
}
